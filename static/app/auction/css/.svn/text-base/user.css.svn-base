/* CSS Document */
#main { background: url(../images/sub_nav_bg.gif) repeat-y;}
#inner { background: url(../images/sub_nav_bg2.gif) no-repeat left bottom;}
#left { width: 163px; float: left; display: inline; border-top: 1px solid #dae2e8; padding: 65px 0 0 0;}
#right { width: 790px; float: right; display: inline; padding: 0 0 30px 0;}

#sub_nav { margin: 0 0 134px 1px;}
#sub_nav ul { background: url(../images/sub_nav_line.gif) no-repeat; padding: 2px 0 0 0;}
#sub_nav li { height: 48px; background: url(../images/sub_nav_line.gif) no-repeat left bottom;}
#sub_nav li a { width: 145px; height: 46px; display: block; line-height: 46px; color: #666; font-size: 14px; padding: 0 0 0 17px; background: url(../images/sub_nav_icon.gif) no-repeat 142px center;}
#sub_nav li.current a { text-decoration: none; background: #fff url(../images/sub_nav_icon.gif) no-repeat 142px center; color: #4d4d4d; font-weight: bold;}
#sub_nav li a:hover { text-decoration: none;}

/*积分规则*/
#rule { padding: 40px 0 0 0;}
#rule h5 { border-bottom: 1px solid #dfdfdf; background: url(../images/rule_h5_icon.gif) no-repeat 10px center; padding: 0 0 0 40px; height: 40px; line-height: 40px; font-size: 14px; font-weight: bold; color: #4d4d4d; margin: 0 0 18px 0;}
#rule p { color: #555; line-height: 24px; font-size: 14px; padding: 0 17px; margin: 0 0 27px 0;}
#rule ul { color: #555; line-height: 28px; font-size: 14px; padding: 0 17px; margin: 0 0 27px 0;}
#rule .ul1 li { background: url(../images/rule_p_icon.gif) no-repeat left center; padding: 0 0 0 20px;}
#rule p.p1 { color: #c04825; margin-bottom: 10px;}
#rule .rule_list { margin-bottom: 0;}
#rule .rule_list li { float: left; display: inline; width: 250px;}
#rule .rule_list li a { color: #0F5FB8;}

/*账户首页*/
#stati { height: 120px; background: #f7f7f7; border: 1px solid #ccc; padding: 0 0 0 28px; margin: 31px 0 22px 0;}
#stati span { font-weight: bold; font-size: 14px; color: #666;}
#stati #sum { margin: 15px 0 19px 0;}
#stati #sum span, #stati #int span { margin: 0 22px 0 0;}
#stati #sum span strong { font-size: 24px; font-family: Tahoma, "宋体"; color: #c00;}
#stati a.hot, #stati a.hot2 { display: inline-block; width: 105px; height: 31px; line-height: 31px; text-align: center; font-size: 14px; font-weight: bold; margin: 0 9px 0 0;}
#stati #sum a.hot2, #stati #int a.hot2 { margin: 0 22px 0 0;}
#stati a.hot:hover, #stati a.hot2:hover { text-decoration: none;}
#stati a.hot { background: url(../images/ad_index_btn.gif) no-repeat; color: #fff;}
#stati a.hot2 { background: url(../images/ad_index_btn2.gif) no-repeat; color: #595959;}
#stati a.a1 { color: #0f5fb8; margin: 0 15px 0 0;}

/*账户共用*/
.safe_box { margin: 0 0 10px 0;}
.safe_box_title { height: 38px; line-height: 38px; background: #f4f6fc;}
.safe_box h4 { color: #4d4d4d; margin: 0 0 0 14px;}
.safe_box_content { border: 3px solid #bcccee; padding: 0 0 23px 0;}
.safe_box .btn button { width: 104px; height: 31px; line-height: 31px; text-align: center; color: #fff; font-size: 14px; font-weight: bold; border: 0; cursor: pointer; background: url(../images/ad_index_btn3.gif) no-repeat;}
.safe_box_content .point { height: 44px; line-height: 44px; border-bottom: 1px dotted #cfd2d7; margin: 0 12px 29px 12px; padding: 0 0 0 8px;}
.safe_box label em { font-style: normal; color: #f00; margin: 0 3px 0 0;}

/*添加银行卡*/
#add_car .data { padding: 0 0 0 44px;}
#add_car .data p { margin: 0 0 19px 0;}
#add_car .data label { margin: 0 3px 0 0;}
#add_car .data span.right { float: left; display: inline; color: #8c8c8c;}
#add_car .data .city select, #add_car .data .num input { position: relative; top: -4px;}
#add_car .data .city label { float: left; display: inline;}
#add_car .data .num label { float: left; display: inline;}
#add_car .data .num input { padding: 2px; width: 324px;}
#add_car .data .btn { padding: 0 0 0 73px;}

/*提取保证金余额到银行卡*/
#cash { padding-bottom: 9px;}
#cash .point { color: #4d4d4d; font-size: 14px; line-height: 34px; padding: 19px 19px 0 15px; height: 34px;}
#cash .point span { float: right; display: inline; font-size: 12px;}
#cash .point span a { color: #0f5fb8;}
#cash .point strong { font-size: 16px; color: #f20000;}
#cash .data p { margin: 0 12px 25px 12px;}
#cash .data p label { width: 131px; text-align: right; display: inline-block; margin: 0 5px 0 0;}
#cash .data .input1 { margin: 0 5px 0 0; *margin: 0 2px 0 0;}
#cash .data .input2 { padding: 2px; width: 93px;}
#cash .p1 span { color: #8c8c8c; margin: 0 0 0 15px;}
#cash .btn { padding: 0 0 0 135px;}

/*确认提现信息*/
#cash2 { padding-bottom: 40px;}
#cash2 .point2 { font-size: 14px; line-height: 30px; background: #f2f2f2; padding: 17px 0 15px 26px; margin: 21px 21px 28px 21px;}
#cash2 .point2 strong span { color: #f20000; font-size: 16px;}
#cash2 .data p { margin: 0 21px;}
#cash2 .data label { display: inline-block; width: 100px; text-align: right; padding: 0 3px 0 0;}
#cash2 .data .input1 { padding: 2px; width: 213px;}
#cash2 .data p.password { margin-bottom: 23px;}
#cash2 .data p.password label { float: left; display: inline; position: relative; top: 5px;}
#cash2 .data p.password span.right { float: left; display: inline; color: #8c8c8c;}
#cash2 .data p.password span.right a { color: #0f5fb8; position: relative; *top: -12px;}
#cash2 .data p.password .input1 { margin: 0 0 8px 0;}
#cash2 .data .btn { padding: 0 0 0 103px;}
#cash2 .data .btn a { color: #0f5fb8; margin: 0 0 0 5px;}

/*提现申请已提交*/
#cash3 { padding: 45px 0 47px 0;}
#cash3 h5 { font-size: 16px; font-weight: bold; background: url(../images/success_icon.gif) no-repeat; padding: 0 0 0 56px; line-height: 40px; margin: 0 0 12px 52px;}
#cash3 p { line-height: 24px; color: #666; margin: 0 0 0 110px;}
#cash3 p strong { font-weight: normal; color: #f00;}

/*提现详细页面*/
.table { width: 95%; margin: 0 auto; border: 0;}
.search_cashdetail { width: 95%; margin: -20px auto 10px auto; padding: 0 0 0 15px; color: #333;}
.search_cashdetail .btn { display: inline;}
.search_cashdetail .btn button { margin-left: 5px; position: relative; top: 2px;}
.search_cashdetail input { padding: 3px;}

/*faq*/
#ad_info_list {}
#ad_info_list li { border-bottom: 1px dashed #d3d3d3;}
#ad_info_list li h5 span { float: right; display: inline; font-size: 12px; color: #666; background: url(../images/ad_icon4.gif) no-repeat left center; padding: 0 0 0 12px; margin: 0 10px 0 0;}
#ad_info_list li h5 a { line-height: 37px; background: url(../images/ad_icon.gif) no-repeat 3px center; padding: 0 0 0 23px; display: block; font-size: 14px; font-weight: normal; color: #464646;}
#ad_info_list li h5 a:hover { text-decoration: none;}
#ad_info_list li .content { background: #f5f5f5; margin: 0 0 18px 0; padding: 22px 18px 13px 28px; line-height: 18px; color: #666;}
#ad_info_list li .btn { text-align: right; padding: 12px 0 0 0;}
#ad_info_list li .btn a { width: 33px; height: 19px; line-height: 19px; display: inline-block; background: #fff url(../images/ad_icon2.gif) no-repeat 8px center; border: 1px solid #e0e0e0; padding: 0 0 0 18px; color: #777; text-align: left;}
#ad_info_list li .btn a:hover { text-decoration: none;}

/*提车及落户信息*/
#set_info { margin: 36px 0 10px 0;}
#set_info h3 { font-size: 18px; font-family: "黑体"; font-weight: normal; border-bottom: 2px solid #808080; height: 30px; line-height: 30px; margin: 0 0 14px 0;}
#set_info .title { height: 36px; line-height: 36px; background: url(../images/ad_title_bg.gif) no-repeat; margin: 14px 0 0 0;}
#set_info .title span { float: right; display: inline;}
#set_info .title span a { width: 33px; height: 19px; line-height: 19px; display: inline-block; background: #fff url(../images/ad_icon2.gif) no-repeat 8px center; border: 1px solid #e0e0e0; padding: 0 0 0 18px; color: #777; margin: 8px 17px 0 0;}
#set_info .title span a.a2 { background-image: url(../images/ad_icon4.gif);}
#set_info .title span a:hover { text-decoration: none;}
#set_info h4 { margin: 0 0 0 11px; background: url(../images/ad_icon3.gif) no-repeat left center; padding: 0 0 0 31px;}
#set_info .content { padding: 30px 0 11px 0; border-bottom: 1px solid #c6c9d7;}
#set_info .form { margin: 0 0 37px 0;}
#set_info .form label { width: 166px; text-align: right; display: inline-block;}
#set_info .form label em { font-style: normal; color: #ff2c04; margin: 0 3px 0 0}
#set_info .form .auto { margin: -2px 0 14px 172px; *margin-top: -6px;}
#set_info .form .auto input { margin: 0 5px 0 0; *margin: 0 2px 0 0;}
#set_info .form .input1 { padding: 3px; border: 1px solid #acacac; width: 174px;}
#set_info .form p { margin: 0 0 11px 0;}
#set_info .form .btn { margin: 0 0 0 172px;}
#set_info .form .btn button { background: url(../images/btn.png) no-repeat; width: 78px; height: 25px; line-height: 25px; color: #fff; font-weight: bold; text-align: center; border: 0; cursor: pointer;}
#set_info h5 { font-size: 12px; font-weight: bold; color: #2e2e2e; padding: 0 0 0 3px; margin: 0 0 13px 0;}
#set_info .table2 { width: 100%; border-collapse: collapse; border: 2px solid #ebebeb;}
#set_info .table2 th { background: #f8f8f8; font-weight: normal;}
#set_info .table2 th, #set_info .table2 td { border: 1px solid #ebebeb; color: #424242; text-align: center; padding: 9px 4px 8px 4px;}

/*信息填写页面*/
#info_fill { padding: 31px 0 0 0;}
#info_fill .point { height: 37px; line-height: 37px; background: #fff7cf; color: #f00; padding: 0 0 0 18px; margin: 0 0 22px 0;}
.info_title { height: 29px; line-height: 29px; background: url(../images/per_info_title_bg.gif) repeat-x left bottom; padding: 0 0 0 20px;}
.info_title a { display: inline-block; width: 147px; height: 29px; line-height: 29px; text-align: center; background: url(../images/per_info_title_tab2.gif) no-repeat; color: #3c86c2; margin: 0 4px 0 0;}
.info_title a.current { background: url(../images/per_info_title_tab.gif) no-repeat; color: #585858; font-weight: bold;}
.info_title a:hover { text-decoration: none;}
.info_content { border: 1px solid #ddd; border-top: 0;}
/*个人信息*/
#per_info { padding: 35px 0 20px 0;}
#per_info p { margin: 0 0 19px 0; zoom: 1;}
#per_info p.type span { margin: 0 0 0 22px;}
#per_info label { display: inline-block; width: 146px; text-align: right; position: relative; *top: -2px;}
#per_info label em { font-style: normal; color: #f58500; margin: 0 3px 0 0;}
#per_info .input1, #per_info #data_fill .input2, #per_info #data_fill .input3 { padding: 2px; width: 176px;}
#per_info .input2 { width: 78px;}
#per_info .input3 { width: 246px;}
#per_info .aut label { float: left; display: inline; margin: 35px 3px 0 0;}
#per_info .aut img { width: 75px; height: 75px; padding: 3px; border: 1px solid #d4d4d4; margin: 0 6px 4px 0;}
#per_info .aut span.right { float: left; display: inline; margin: 0 23px 0 0;}
#per_info .aut em { font-style: normal; display: block;}
#per_info .aut button { position: relative; top: -35px; padding: 2px 5px; cursor: pointer;}
#per_info .about textarea { width: 396px; height: 102px;}
#per_info .about label { position: relative; top: -45px;}
#per_info .btn { padding: 2px 0 0 151px;}
#per_info .btn button { background: url(../images/btn.png) no-repeat; width: 78px; height: 25px; line-height: 25px; border: 0; color: #fff; text-align: center; cursor: pointer; margin: 0 12px 0 0; font-weight: bold;}
#per_info .btn button.b2 { background: url(../images/btn2.png) no-repeat; color: #686868;}
/*信息订阅*/
#subscription { color: #686868; padding: 27px 0 46px 0;}
#subscription .part1 { border-bottom: 2px solid #efefef; margin: 0 32px 20px 32px; line-height: 24px; padding: 0 21px 15px 21px;}
#subscription h5 { font-size: 12px; font-weight: bold; color: #505050; margin: 0 0 7px 0;}
#subscription .part1 .input1 { margin: 0 5px 0 0; *margin: 0 2px 0 0;}
#subscription .btn { padding: 0 0 0 52px; margin: 0 0 26px 0;}
#subscription .btn button { background: url(../images/btn.png) no-repeat; width: 78px; height: 25px; line-height: 25px; text-align: center; color: #fff; font-weight: bold; border: 0; cursor: pointer;}
#subscription .add_sub a { background: url(../images/subscription_icon.gif) no-repeat left center; padding: 0 0 0 17px; color: #0f5fb8;}
#subscription .add_sub { padding: 0 0 0 39px; margin: 0 0 11px 0;}
#subscription .add_sub_content { width: 379px; border: 1px solid #ccddea; margin: 0 0 0 40px; padding: 0 0 9px 0;}
#subscription .third_title { background: #f4f7f9; padding: 9px 0 9px 18px;}
#subscription .third_title select, #subscription .sub_title button { font-size: 13px; margin: 0 5px 0 0;}
#subscription .add_sub_content li { border-bottom: 1px solid #e4e4e4; margin: 0 9px; padding: 11px 0 12px 11px; color: #767676;}
#subscription .add_sub_content li span { float: right; display: inline; padding: 0 15px 0 0;}
#subscription .add_sub_content li span a { color: #0f5fb8; margin: 0 0 0 20px;}
#subscription .add_sub_content li.li_title { color: #333;}
/*手机号码设置*/
#phone_set { padding: 40px 0 40px 0; color: #000;}
#phone_set h5 { font-size: 14px; font-weight: bold; margin: 0 0 0 36px;}
#phone_set button { background: url(../images/btn.png) no-repeat; width: 78px; height: 25px; line-height: 25px; color: #fff; font-weight: bold; text-align: center; border: 0; cursor: pointer;}
#phone_set .part1 { margin: 0 0 47px 0;}
#phone_set .part1 h5 { margin-bottom: 6px;}
#phone_set .part1 p { padding: 0 0 0 50px;}
#phone_set .part1 p span { margin: 0 4px;}
#phone_set .part2 h5 { margin-bottom: 27px;}
#phone_set .part2 ul { margin: 0 0 8px 78px;}
#phone_set .part2 li { line-height: 27px;}
#phone_set .part2 li span { color: #858585; margin: 0 0 0 30px;}
#phone_set .part2 li a { color: #0f5fb8;}
#phone_set .part2 .btn { margin: 0 0 0 77px;}

/*我关注过的车*/
#follow_car { margin: 33px 0 0 0;}
#follow_car .user_table_title { background: url(../images/user_table_title_bg.gif) repeat-x left bottom; height: 29px; line-height: 29px; padding: 0 0 0 20px;}
#follow_car .user_table_title h5 { width: 140px; height: 29px; background: url(../images/user_table_title_tab.gif) no-repeat; font-weight: bold; color: #585858; text-align: center; line-height: 29px; font-size: 12px;}

.user_search { height: 75px; border: 1px solid #ccddea; border-top: 0; background: url(../images/user_search_bg.gif) repeat-x;}
.user_search label { float: left; display: inline; width: 71px; padding: 0 0 0 21px; line-height: 74px;}
.user_search .select { width: 508px; float: left; display: inline; padding: 15px 0 0 0;}
.user_search .select select { width: 110px; margin: 0 6px 5px 0;}
.user_search button { background: url(../images/btn.gif) no-repeat; width: 75px; height: 25px; line-height: 25px; font-weight: bold; color: #595959; text-align: center; border: 0; cursor: pointer; margin: 25px 0 0 0;}

.user_table { border-collapse: collapse; width: 100%; color: #404040; line-height: 20px;}
.user_table th, .user_table td { border: 1px solid #ddd; text-align: center; padding: 17px 5px;}
.user_table th { background: #edf4fd; font-weight: normal; padding-top: 8px; padding-bottom: 8px; border-left: 1px solid #edf4fd; border-right: 1px solid #edf4fd; border-top: 0; line-height: normal;}
.user_table th.first { border-left: 1px solid #ddd;}
.user_table th.left { text-align: left; padding: 0 0 0 16px; border-left: 1px solid #ddd;}
.user_table th.last { border-right: 1px solid #ddd;}
.user_table dl { text-align: left; margin: 0 0 0 13px;}
.user_table dt { float: left; display: inline;}
.user_table dt img { width: 79px; height: 59px; padding: 2px; border: 1px solid #d1d1d1;}
.user_table dd { float: left; display: inline; width: 252px; margin: 0 0 0 13px;}
.user_table a { color: #0f5fb8;}
.user_table .price { font-weight: bold;}
.user_table .price strong { font-size: 14px;}
.user_table .yellow { background: url(../images/user_table_yellow.gif) no-repeat; display: inline-block; width: 71px; height: 23px; line-height: 23px; text-align: center; color: #d1a343; margin: 3px 0 0 0;}
.user_table em { font-style: normal; color: #a1a1a1;}
.user_table .orange { display: inline-block; background: url(../images/table_orange.gif) no-repeat; margin: 0 0 5px 0;}
.user_table .orange a { display: inline-block; line-height: 21px; background: url(../images/table_orange.gif) no-repeat right -27px; padding: 0 8px 0 9px; color: #fff;}
.user_table .orange a:hover { text-decoration: none;}
.user_table .gray { background: #f7f7f7;}
.user_table .gray #page { text-align: right; padding: 0 13px 0 0;}
.user_table .gray #page button { line-height: normal;}
.user_table .c1, .user_table .c2 { background: url(../images/user_table_icon.gif) no-repeat left top; display: inline-block; padding: 0 0 0 22px;}
.user_table .c2 { background-position: left -28px;}
.user_table p.p1, .user_table p.view { width: 197px; text-align: left; margin: 0 0 0 8px;}
.user_table p.p1 { margin-bottom: 10px;}

/*买到的车*/
#buy_car { padding: 31px 0 0 0;}
#buy_car #remind { line-height: 24px; background: #f8f8f8; border: 1px solid #d2d2d2; font-size: 14px; color: #333; padding: 31px 0 31px 34px; margin: 0 0 15px 0;}
#buy_car #remind span { margin: 0 0 0 21px;}
#buy_car #remind strong { color: #f00; font-weight: bold;}

#buy_car .user_search { border: 1px solid #ccddea;}
#buy_car .user_search .select { width: 598px;}
#buy_car .user_search .select select { width: 110px; margin-right: 0;}

/*新增_20130222*/
#order_detail { margin: 33px 0 0 0;}
#order_detail #process { border: 1px solid #b7cee8; margin: 0 0 14px 0;}
#order_detail #process .title { background: #f4f8fc; height: 38px; line-height: 38px; padding: 0 0 0 12px; color: #3e3e3e;}
#order_detail #process .title em { float: right; display: inline; font-style: normal; margin: 0 13px 0 0;}
#order_detail #process .title em a { color: #3b73a6; padding: 0 0 0 12px; background: url(../images/order_detail_icon.gif) no-repeat left center;}
#order_detail #process .title span { margin: 0 24px 0 0;}
#order_detail #process .title strong { color: #c00; font-size: 14px;}
#order_detail #process .content { padding: 17px 0;}
#order_detail #process .content .pic { height: 55px; background: url(../images/order_detail_step.png) no-repeat;}
#order_detail #process .content .pic2 { height: 55px; background: url(../images/order_detail_step.png) no-repeat center 2px;}
#order_detail #process .content .pic3 { height: 55px; background: url(../images/order_detail_step.png) no-repeat center -88px;}
#order_detail #process .content .pic4 { height: 55px; background: url(../images/order_detail_step.png) no-repeat center -168px;}
#order_detail #process .content ul { padding: 5px 0 0 15px;}
#order_detail #process .content li { float: left; display: inline; width: 190px; text-align: center; line-height: 18px; color: #9bb9d8;}
#order_detail #process .content li span { display: block;}
#order_detail #process .content li.current { color: #003884;}
#order_detail #process .content li.current span { color: #4a6fb7;}

#order_detail #tips { height: 93px; overflow: hidden; background: #fff8ee; border: 1px solid #f3b96a; color: #404040; clear: both; padding: 21px 0 0 25px; position: relative; margin: 0 0 15px 0;}
#order_detail #tips h5 { font-size: 14px; font-weight: bold; color: #404040; margin: 0 0 17px 0;}
#order_detail #tips p { line-height: 22px;}
#order_detail #tips strong { color: #f50; font-weight: normal;}
#order_detail #tips .appeal { position: absolute; right: 15px; bottom: 16px;}

#order_detail #para { margin: 0 0 12px 0; color: #404040;}
#order_detail #para .title { background: url(../images/order_detail_title_bg.gif) repeat-x left bottom; color: #333;}
#order_detail #para .title h4 { height: 27px; background: url(../images/order_detail_title_bg2.gif) no-repeat; width: 130px; line-height: 27px; text-align: center; margin: 0 0 0 20px;}
#order_detail #para .content { border: 1px solid #aec7e5; border-top: 0; padding: 29px 0 0 0;}
#order_detail #para .part1 { margin: 0 18px 15px 18px; border-bottom: 1px solid #d1d7dc; padding: 0 0 7px 0;}
#order_detail #para h5 { font-size: 12px; font-weight: bold; margin: 0 0 14px 3px; clear: both;}
#order_detail #para h5 em { font-style: normal; font-weight: normal;}
#order_detail #para p, #order_detail #para .part1 dl { line-height: 30px;}
#order_detail #para .part1 dl { clear: both; margin: 0 0 0 35px;}
#order_detail #para .part1 dt, #order_detail #para .part1 dd { float: left; display: inline;}
#order_detail #para .part1 dt { width: 110px; text-align: right; margin: 0 4px 0 0;}
#order_detail #para .part1 dd { width: 220px;}
#order_detail #para #sec p { margin: 0 0 0 73px;}
#order_detail #para #sec p em { font-style: normal; margin: 0 0 0 18px;}
.order_detail_table1 { width: 100%; border-collapse: collapse; border: 1px solid #ddd; line-height: 23px; margin: 0 0 12px 0;}
.order_detail_table1 th { border-right: 1px solid #f3f3f3; background: #f3f3f3; text-align: center; padding: 6px 0 3px 0; font-weight: normal;}
.order_detail_table1 th.last { border-right: 1px solid #ddd;}
.order_detail_table1 tr.blue th { background: #e8f2ff; border-right: 1px solid #e8f2ff;}
.order_detail_table1 tr.blue th.last { border-right: 1px solid #ddd;}
.order_detail_table1 td { border: 1px solid #ddd; text-align: center; padding: 6px 0 3px 0;}
.order_detail_table1 td.left { text-align: left; padding-left: 13px;}
.order_detail_table1 a { color: #0f5fb8;}
.order_detail_table1 .hot { color: #f58500;}
#order_detail #para #modify .order_detail_table1 { margin: 0 0 20px 0;}
