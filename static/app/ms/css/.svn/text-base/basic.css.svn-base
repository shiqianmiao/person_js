.title_style { background: url(../images/title_bg.gif) repeat-x left bottom; height: 34px;}
.bgcolor-f9{background:#f9f9f9;}
.title_style h2,.title_style h3 { float: left; display: inline; color: #494949; font-size: 14px; font-family: Microsoft YaHei;}
.title_style h2 strong,.title_style h3 strong { display: inline-block; height: 32px; line-height: 32px; padding: 0 21px; border-bottom: 2px solid #aaa; position: relative;}
.title_style h2 strong,.title_style h3 strong i { background: #b3d077 url(../images/rp.png) no-repeat 0 -800px; display: inline-block; height: 18px; color: #fff; position: absolute; top: 7px; right: -122px; font-size: 12px; font-style: normal; line-height: 18px; font-weight: normal; font-family: "宋体"; padding: 0 6px 0 10px; width: 122px;}
.title_style h2 strong,.title_style h3 strong i b { display: inline-block; width: 5px; height: 18px;}
.title_style h2 a,.title_style h3 a { display: inline-block; height: 32px; line-height: 31px; padding: 0 17px; border-bottom: 2px solid #aaa; cursor: pointer; position: relative;}
.title_style h2 .current,.title_style h3 .current { color: #f37c06; border-bottom: 2px solid #f37c06; background: url(../images/title_bg2.gif) no-repeat center bottom;}
.title_style h2 .current:link,.title_style h3 .current:link, .title_style h3 .current:visited { color: #f37c06;}
.title_style h2 a:hover,.title_style h3 a:hover { text-decoration: none;}
.title_style h2 a em,.title_style h3 a em { position: absolute; top: -3px; right: -1px;}
.title_style .more { float: right; display: inline; line-height: 32px; margin: 0 10px 0 0;}
