#page404 .tishi,#page404 p{width:390px;margin:auto;line-height:24px;}
#page404 .tishi b{color:#F37C06;}
#page404 p{background:url(../images/dot.png) no-repeat 0 center;text-indent:10px;}
#page404 p a{color:#F37C06;}

/*Map tips*/
.map-tips{display:block;height:26px;background:url(../images/map-tips.png) no-repeat 0 -28px;cursor:pointer;}
.map-tips span.zimu{width:20px;text-align:center;line-height:18px;color:#fff;font-size:14px;float:left;}
.map-tips span.name{display:none;}
.map-tips span.name a{color:#fff;}
.map-tips span.name a:hover{text-decoration:underline;}
.map-tips-on span.zimu{width:17px;}
.map-tips-on span.name{display:block !important;float:left;height:26px;line-height:20px;color:#fff;padding:0 5px;background:url(../images/map-tips.png) no-repeat right 0;}
.map-ckb .map-tips{background:url(../images/map-tips-ckb.png) no-repeat 0 -28px;}
.map-ckb .map-tips-on span.name{background:url(../images/map-tips-ckb.png) no-repeat right 0;}


/*撤销图标*/
#index_left span.com_authen_icon,#index_left span.com_authen_icon2,
#car_list div.authen_icon,
.maybe_like span.com_authen_icon,.maybe_like span.com_authen_icon2,
.com_list span.com_authen_icon,.com_list span.com_authen_icon2,
.pic span.com_authen_icon,.pic span.com_authen_icon2,
ul.car_list span.com_authen_icon,ul.car_list span.com_authen_icon2
{display:none !important;}
#car_list .price{float:right !important;margin-right:20px !important;}

/*顾问已看车*/
span.gwykc,span.xszysh{background:#71A6D1;padding:2px;color:#fff !important;font-size:12px;}

/*temp_msg*/
.temp_msg{position:relative; background:#FFF3C0;width:100%;min-width:1000px;text-align:center;color:#d00;font-weight:700;line-height:30px;font-size:14px;border-bottom:1px #D0CAB0 solid;}
.temp_msg span.close{text-indent:-82982px;position:absolute;top:5px;right:20px;font-weight:100;color:#333;display:inline-block;width:19px;height:19px;cursor:pointer;background:url(../images/p.png) no-repeat -110px -270px;}

/*车况宝图标*/
.icons-ckb-l,.icons-ckb-m,.icons-ckb-s{display:block;position:absolute;top:8px;left:10px;}
.icons-ckb-l{width:80px;height:66px;background:url(../images/icons-ckb.png) no-repeat 0 0;}
.icons-ckb-m{width:50px;height:41px;background:url(../images/icons-ckb.png) no-repeat 0 -70px;}
.icons-ckb-s{width:40px;height:32px;background:url(../images/icons-ckb.png) no-repeat 0 -120px;}
.icons-ckb-l,.icons-ckb-m,.icons-ckb-s{_background-image:url(../images/icons-ckb-ie6.png);}

/*7天无理由退车图标*/
.icons-7days-m,.icons-7days-m2,.icons-7days-l{display:block;position:absolute;top:8px;left:10px;}
.icons-7days-m{ width: 36px; height: 38px; background:url(../images/icons-7days-m.png) no-repeat;}
*html .icons-7days-m {background:none; filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../images/icons-7days-m.png');}
.icons-7days-m2{ width: 48px; height: 56px; background:url(../images/icons-7days-m2.png) no-repeat;}
*html .icons-7days-m2 {background:none; filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../images/icons-7days-m2.png');}
.icons-7days-l{ width: 59px; height: 70px; background:url(../images/icons-7days-l.png) no-repeat;}
*html .icons-7days-l {background:none; filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../images/icons-7days-l.png');}

/*新增_集市问答列表_20131202*/
.faq_box { line-height: 24px; height: 318px; overflow: hidden;}
.faq_box ul { padding: 4px 0 0 0;}
.faq_box li { margin: 0 9px 0 14px; border-bottom: 1px dashed #e3e3e3; padding: 7px 0 5px 0; clear: both;}
.faq_box li.last { border: 0;}
.faq_box li dt { float: left; display: inline; width: 14px; height: 14px; background: url(../images/rp.png) no-repeat 0 -1460px; margin: 4px 0 0 0;}
.faq_box li dd { float: left; display: inline; width: 180px; margin: 0 0 0 9px;}
.faq_box li .dl1 dd { font-weight: bold;}
.faq_box li .dl2 dt { background-position: -20px -1460px;}

/*新增_新闻动态样式_20131202*/
.news_list li { line-height: 24px; font-size: 14px; color: #989898;}
.news_list li i { display: inline-block; width: 4px; height: 24px; background: url(../images/rp.png) no-repeat 0 -375px; margin: 0 7px 0 0; vertical-align: middle;}
.news_list li span { float: right; display: inline;}
.news_list li.line { height: 1px; line-height: 1px; overflow: hidden; border-bottom: 1px dashed #e7e7e7; margin: 12px 0 14px 0;}

/*遮罩层与弹出框_20131202*/
.mask_layer { position: absolute; top: 0; left: 0; width: 100%; height: 100%; background: #000; filter: Alpha(Opacity=50); opacity:0.5; z-index: 10;}
.pop_box {width:410px;border:5px #8E8E8F solid;position:absolute;top:0;left:0;background:#FCFCFC; z-index: 99;}
.pop_box h4{background:#f0f0f0;height:26px;line-height:26px;padding:2px 20px;font-size:14px;position:relative;}
.pop_box h4 span.close{display:block;width:30px;height:30px;background:url(../images/p.png) no-repeat -140px -270px;text-indent:-82982px;right:0;top:0;position:absolute;cursor:pointer;}
.pop_box .content { padding: 27px 0 25px 0;}
.pop_box .input_box { margin: 0 0 13px 0;}
.pop_box label { display: inline-block; width: 89px; text-align: right; font-size: 14px; color: #a3a3a3; padding: 0 13px 0 0;}
.pop_box label em { font-style: normal; color: #e20000; margin: 0 6px 0 0;}
.pop_box .input1 { font-size: 12px; padding: 5px 5px 4px 5px; border: 1px solid #cecece; width: 222px;}
.pop_box .select1 { width: 112px; margin: 0 10px 0 0;}
.pop_box .btn { margin: 0 0 10px 0;}
.pop_box .btn button { margin: 0 10px 0 0; font-size:14px; width:110px; height:28px; line-height:28px; border:1px #F8CEA3 solid; background:#FFECD7; color:#F37C06; cursor:pointer; font-weight:700;}
.pop_box .btn button.cancel{border-color:#cecece;background:#f2f2f2;color:#888;}
.pop_box .warm_tips { color: #a3a3a3; line-height: 18px; padding: 3px 34px 0 34px;}
.pop_box .bottom_tips { font-size: 14px; line-height: 40px; background: #efefef;}
.pop_box .bottom_tips p { margin: 0 0 0 20px;}
.pop_box .bottom_tips i { display: inline-block; width: 14px; height: 40px; vertical-align: middle; background: url(../images/rp.png) no-repeat left -1480px; margin: 0 9px 0 0;}
.pop_box .bottom_tips strong { color: #ff7e00;}

.icon-daikuangouche{display:inline-block;width:100px;height:24px;background:url(../images/p.png) no-repeat 0 -320px;position: relative;top: 3px}