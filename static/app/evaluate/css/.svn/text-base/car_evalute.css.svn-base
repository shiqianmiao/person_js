#eva_list .filter .morelayer { background: #fff; position:absolute;left:-1px;top:-1px; border:#cfcfcf solid 1px; box-shadow:#cfcfcf 1px 1px 5px;}
#eva_list .filter dl { margin: 0 14px; line-height: 24px; padding:10px 0;}
#eva_list .filter dt { float: left; display: inline; padding: 0 0 0 5px; width: 56px; color:#989898;}
#eva_list .filter dd { float: left; display: inline; position:relative; width: 80px; border: 1px solid #cecece; background: url(../images/rp.png) no-repeat 100% -232px;position:relative;z-index:0; height: 20px; display: inline-block; line-height: 20px; margin: 0 15px 0 0;}
#eva_list .filter dd a { padding: 0 5px; line-height: 20px; display: block; zoom: 1;}
#eva_list .filter dd a:hover { text-decoration: none;}
#eva_list .filter dd a:visited { color: #494949; text-decoration: none;}
#eva_list .filter dd .morelayer { display: none;}
#eva_list .filter dd.current { border: 1px solid #ff8400; color: #f37c06; text-decoration: none; background:url(../images/rp.png) no-repeat 100% -272px;}
#eva_list .filter dd.current a { color: #f37c06;}
#eva_list .filter dd.current .morelayer { display: block;}
#eva_list .filter dd.current a:visited { color: #f37c06;}
#eva_list .filter dd.current .morelayer a { color: #494949;}
#eva_list .filter dd.current .morelayer a:visited { color: #494949;}
#eva_list .filter dd.current .morelayer a:hover { color: #fff;}
#eva_list .filter .morelayer .title { padding: 0 14px 0 5px; background: url(../images/rp.png) no-repeat 100% -232px; color: #494949;}
#eva_list .filter dd.dd1, #eva_list .filter dd.dd1 .morelayer ul { width: 100px;}
#eva_list .filter dd.dd2, #eva_list .filter dd.dd2 .morelayer ul { width: 100px;}
#eva_list .filter dd.dd3, #eva_list .filter dd.dd3 .morelayer ul { width: 240px;}
#eva_list .filter dd.dd3 .morelayer .title { width: 221px; height: 20px; overflow: hidden;}
#eva_list .filter dd.dd3 a { width: 220px; height: 20px; overflow: hidden; display: block;}
#eva_list .filter dd.dd3 .morelayer a { width: 210px;}

#eva_list .filter .morelayer ul li a{position:static!important;background:none;border:0;word-break:keep-all;margin-left:5px;}
#eva_list .filter dd .morelayer ul li a{background:#fff;border:0;margin:0 1px;}
#eva_list .filter dd .morelayer ul li a:hover{background:#f37c06;color:#fff;border:0;}
#eva_list .filter dd .morelayer ul { height: 450px; overflow-y: scroll;}

