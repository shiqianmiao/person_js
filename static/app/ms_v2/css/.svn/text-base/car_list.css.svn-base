/*筛选列表*/
.filter_list {
  font-size: 14px;
  border-bottom: 1px solid #ebebeb;
}
.filter_list h4 {
  float: left;
  _display: inline;
  width: 71px;
  line-height: 24px;
  text-align: right;
  color: #989898;
}
.filter_list .selected {
  background: #f9f9f9;
  padding: 9px 0 4px;
  border-width: 1px 1px 0 1px;
  border-style: solid;
  border-color: #ebebeb;
}
.filter_list .selected h4 {
  width: 75px;
}
.filter_list .selected .con {
  float: left;
  _display: inline;
  width: 1090px;
  padding-left: 15px;
}
.filter_list .selected .con span {
  float: left;
  position: relative;
  padding: 0 3px;
  border: 1px solid #fc5300;
  color: #fc5300;
  margin: 0 10px 5px 0;
  height: 22px;
  line-height: 22px;
  cursor: pointer;
}
.filter_list .selected .con span a {
  display: block;
  position: absolute;
  top: 4px;
  right: 3px;
  width: 14px;
  height: 14px;
  background: #f9f9f9 url(../images/icons.png) no-repeat -420px -610px;
}
