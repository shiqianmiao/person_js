}
.filter_list .selected .del {
  float: left;
  _display: inline;
  overflow: hidden;
}
.filter_list .selected .del a {
  position: relative;
  padding-left: 16px;
  color: #0090d8;
  line-height: 24px;
}
.filter_list .selected .del a .i-del {
  display: block;
  width: 14px;
  height: 14px;
  position: absolute;
  top: 2px;
  background: #f9f9f9 url(../images/icons.png) no-repeat -360px -610px;
  overflow: hidden;
}
.filter_list .selected .save {
  float: right;
  _display: inline;
  position: relative;
  height: 22px;
  z-index: 7;
  color: #d6d6d6;
}
.filter_list .selected .save a.a2 {
  color: #0090D8;
  margin: 0 0 0 3px;
}
.filter_list .selected .save a.a1 {
  color: #0090d8;
  line-height: 22px;
  margin: 0 5px;
}
.filter_list .selected .save a.a1 .i-save {
  display: block;
  width: 14px;
  height: 14px;
  position: absolute;
  top: 6px;
  left: -11px;
  background: #f9f9f9 url(../images/icons.png) no-repeat -760px -280px;
  overflow: hidden;
}
.filter_list .selected .save .arrow {
  width: 13px;
  height: 7px;
  background: url(../images/icons.png) no-repeat -630px -610px;
  position: absolute;
  top: 23px;
  right: 29px;
  z-index: 2;
}
.filter_list .selected .save .down {
  width: 435px;
  background: #fff;
  border: 1px solid #cfcfcf;
  color: #494949;
  position: absolute;
  top: 29px;
  right: -9px;
  z-index: 1;
  font-family: SimSun;
  font-size: 14px;
  box-shadow: 1px 1px 5px #bbb;
}
.filter_list .selected .save .down li {
  margin: 0 2px;
  border-bottom: 1px solid #efefef;
}
.filter_list .selected .save .down li .li_c {
  height: 36px;
  /*background: #ebf3f7;*/

  line-height: 36px;
  margin: 2px 0;
  cursor: pointer;
}
.filter_list .selected .save .down li .li_c i.i-s {
  float: left;
  display: inline-block;
  width: 12px;
  height: 12px;
  background: url(../images/icons.png) no-repeat -600px -610px;
  margin: 12px 8px 0 9px;
}
.filter_list .selected .save .down li .li_c em.font {
  float: left;
  display: inline;
  width: 370px;
  height: 36px;
  overflow: hidden;
}
.filter_list .selected .save .down li .li_c a.close {
  float: right;
  display: inline-block;
  width: 15px;
  height: 15px;
  background: url(../images/icons.png) no-repeat -538px -608px;
  padding: 0;
  margin: 10px 8px 0 0;
}
.filter_list .selected .save .down li .li_c a.close:hover {
  background: #0090d8 url(../images/icons.png) no-repeat -568px -608px;
}
.filter_list .selected .save .down li .li_c_on {
  background: #ebf3f7;
}
.filter_list .selected .save .down li.last {
  border-bottom: 0;
}
.filter_list .selected .save .tips {
  position: absolute;
  top: 12px;
  right: 43px;
  z-index: 1;
  width: 483px;
  height: 221px;
  background: url(../pics/car_list_tips.png) no-repeat;
}
.filter_list .selected .save .tips .tips_inner {
  position: relative;
  width: 483px;
  height: 221px;
}
.filter_list .selected .save .tips .tips_inner a {
  display: block;
  position: absolute;
  top: 145px;
  left: 147px;
  width: 117px;
  height: 52px;
}
