/*reset*/
@charset "utf-8";
audio,canvas,video{display:inline-block;*display:inline;*zoom:1;}
audio:not([controls]){display:none;height:0;}
[hidden]{display:none;}
html,button,input,select,textarea{font-size:100%;}
html{-webkit-text-size-adjust:100%;-ms-text-size-adjust:100%;}
body,div,dl,dt,dd,ul,ol,menu,li,h1,h2,h3,h4,h5,h6,p,pre,code,form,fieldset,legend,button,input,select,textarea,blockquote,figure{margin:0;padding:0;}
body{font:0.875em/1.5 "Microsoft Yahei";color:#333;}
a{text-decoration:none;color:#333;}
a:visited{color:#333;}
a:focus,a:active,a:hover{outline:none;}
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}
sub,sup{position:relative;vertical-align:baseline;}
ul,ol,li{list-style:none;list-style-image:none}
img{border:0;-ms-interpolation-mode:bicubic;max-width:100%;}
/*svg:not(:root){overflow:hidden}*/
fieldset{border:0;}
legend{border:0;white-space:normal;*margin-left:-7px;}
button,input,select,textarea{vertical-align:baseline;*vertical-align:middle;outline:none;}
button,input{line-height:normal;border-radius:0;}
button,input[type="button"],input[type="reset"],input[type="submit"]{-webkit-appearance:button;cursor:pointer;*overflow:visible;}
button[disabled],input[disabled]{cursor:default;}
input[type="checkbox"],input[type="radio"]{box-sizing:border-box;*height:13px;*width:13px;}
input[type="search"]{-webkit-appearance:textfield;-moz-box-sizing:content-box;-webkit-box-sizing:content-box;box-sizing:content-box;}
input[type="search"]::-webkit-search-cancel-button,input[type="search"]::-webkit-search-decoration{-webkit-appearance:none;}
textarea{overflow:auto;vertical-align:top;resize:none;}
table{border-collapse:collapse;border-spacing:0;}
/** common styles */
.clearfix:before,.clearfix:after{content:' ';display:table;clear:both;}
.clearfix{*zoom:1}
body{background:#eee;min-width:320px;}
.view-more{margin:7px;height:36px;line-height:36px;border:1px solid #c7c7c7;text-align:center;}
.view-more a{display:block;background-image:-moz-linear-gradient(top, #f1f1f1, #eaeaea);background-image:-webkit-linear-gradient(top, #f1f1f1, #eaeaea);}
.top-area{height:24px;position:absolute;top:10px;right:10px;background:#ff7900 url(icon-city.png) no-repeat 5px center;background-size:auto 15px;}
.top-area .current{width:36px;height:23px;padding:0 0 0 20px;line-height:23px;color:#fff;}
.bread .top-area{position:absolute;margin:0;top:10px;right:5px;background-color:transparent;}
/* header*/
header{padding:10px 0;position:relative;border-bottom:1px #cc2a48 solid;background:#f70;background:-webkit-gradient(linear,0 0,0 100%,from(#FF7D01),to(#FF7101));background:-moz-linear-gradient(top,#FF7D01,#FF7101);}
header .logo{width:170px;height:40px;background:url(logo.png) no-repeat 8px 0;background-size:160px auto;}
header .logo a{display:block;height:100%;text-indent:-82982em;}
header .logo a:hover{background:none;}
.city{position:absolute;top:18px;right:10px;}
bread .city{top:10px;}
.city a{text-indent:15px;min-width:50px;display:block;color:#fff;background:url(icon-city.png) no-repeat 5px center;background-size:10px auto;padding:2px 5px;height:20px;font-size:1.1em;line-height:20px;}
.city a:hover{background-color:#f30;box-shadow:1px #f70;border-radius:4px;}
.search{display:block;height:32px;clear:both;padding:5px 10px 0;}
.search form{height:30px;background:#fff;position:relative;display:-webkit-box;-webkit-box-flex:1;}
.search form input[type="text"]{padding:3px;margin:3px 42px 0 0;display:-webkit-box;-webkit-box-flex:1;border:0;-webkit-appearance: none;}
.search form input[type="submit"]{text-indent:-82982em;width:42px;height:30px;position:absolute;top:0;right:0;background:#eaeaea url(icon-search.png) no-repeat 0 0;background-size:auto 32px;border:0;border-left:1px solid #dbdbdb;}
.search form.adv{border-right:50px #FF7000 solid;}
.search form.adv input[type="text"]{background:url(icon-search-hui.png) no-repeat 5px center;background-size:17px auto;text-indent:20px;}
.search form.adv input[type="submit"]{padding-bottom:2px;right:-50px;background:#FF7500;border:1px #DB5D00 solid;text-indent:0;text-align:center;color:#fff;}
typetab{width:100%;min-width:320px;}
typetab ul#typetab{height:75px;}
typetab ul#typetab li{float:left;width:25%;min-width:80px;position:relative;}
typetab ul#typetab li s{display:none;position:absolute;bottom:0;text-decoration:none;width:100%;background:url(icon-s.png) no-repeat center bottom;background-size:auto 5px;}
typetab ul#typetab li.on s{display:block;}
typetab ul#typetab li.on span{border-bottom:1px #FF6F00 solid;}
typetab ul#typetab li span{display:block;height:25px;text-align:center;padding-top:50px;cursor:pointer;border-right:#e2e2e2 solid 1px;border-left:#fff solid 1px;}
typetab ul#typetab li span.cx{background:#f4f4f4 url(icon-type-cx.png) no-repeat center;background-size:35px auto;}
typetab ul#typetab li span.jg{background:#f4f4f4 url(icon-type-jg.png) no-repeat center;background-size:35px auto;}
typetab ul#typetab li span.rm{background:#f4f4f4 url(icon-type-rm.png) no-repeat center;background-size:35px auto;}
typetab ul#typetab li span.ss{background:#f4f4f4 url(icon-type-ss.png) no-repeat center;background-size:35px auto;}
typetab ul#typetab li span a{color:#333;display:block;position:absolute;top:2px;left:5%;width:90%;height:22px;padding-top:48px;}
typetab .typetab-data{border-top:1px #C6C6C6 solid;width:100%;display:none;}
typetab ul.typetab-data li{float:left;width:25%;}
typetab ul.typetab-data li span a{background:#e8e8e8;display:block;height:36px;line-height:36px;text-align:center;border-right:#cdcdcd solid 1px;border-bottom:#cdcdcd solid 1px;box-shadow:inset 1px 1px #f4f4f4;}

carlist{clear:both;overflow:hidden;display:block;}
carlist ul{width:100%;}
carlist li{min-width:300px;margin:10px;height:75px;background:#f9f9f9;border:1px solid #d4d4d4;clear:both;overflow:hidden;position:relative;}
carlist li .pic{width:100px;margin-right:10px;float:left;}
carlist li .info{float:left;width:60%;padding:2px 0 0 0;}
carlist li .info p{color:#bbb;}
carlist h3{font-size:1.1em;width:90%;overflow:hidden;height:1.5em;}
carlist li a{display:block;}
carlist span.price{color:#c00;font-size:1.2em;position:absolute;bottom:5px;right:10px;}
carlist .more{margin:10px;height:36px;line-height:36px;border:1px solid #c7c7c7;text-align:center;}
carlist .more a{display:block;background-image:-moz-linear-gradient(top,#f1f1f1,#eaeaea);background-image:-webkit-linear-gradient(top,#f1f1f1,#eaeaea);}

.carlist-tab{border:1px #dfdfdf solid;border-bottom:0;background:#f4f4f4;height:40px;}
.carlist-tab span{float:left;text-align:center;border-bottom:1px #dfdfdf solid;width:50%;line-height:39px;height:39px;}
.carlist-tab span a{display:block;width:100%;height:39px;}
.carlist-tab span.on{border-bottom:4px #FF6900 solid;line-height:36px;height:36px;width:49%;}
.carlist-tab em{float:left;width:0px;overflow:hidden;border-right:1px #dfdfdf solid;line-height:40px;height:40px;}

/*面包屑导航*/
bread{display:block;height:42px;line-height:42px;background:#ff7a00;background-image:-moz-linear-gradient(top,#ff7d00,#ff6700);background-image:-webkit-linear-gradient(top,#ff7d00,#ff6700);border-bottom:2px solid #cc6100;color:#fff;position:relative;}
bread a{color:#fff;display:inline-block;padding:0 5% 0 4%;}
bread a:visited{color:#fff;}
bread .first{background:url(bread-bg.png) no-repeat right center;background-size:auto 42px;}
bread .second{background:url(bread-bg2.png) no-repeat right center;background-size:auto 42px;}
bread span{margin:0 0 0 2.5%;}
bread .bread-right{float:right;width:9.5%;height:30px;background:#d96600;background-image:-moz-linear-gradient(top,#d96a00,#d95700);background-image:-webkit-linear-gradient(top,#d96a00,#d95700);border-radius:4px;position:absolute;top:5px;right:3%;border:1px solid #a64200;box-shadow:0 1px #ffb481;}
bread .bread-right a{background:url(search-icon2.png) no-repeat center center;background-size:auto 22px;display:block;height:30px;}
bread span.search{position:absolute;top:0;right:0;}
bread span.search a{display:block;width:30px;height:30px;border:1px #C25800 solid;border-radius:5px;background:#D96900 url(icon-search-white.png) no-repeat center;background-size:20px auto;text-indent:-82982em;}
/*搜索*/
.search-list li{margin:7px;background:#fff;height:35px;line-height:35px;border:1px solid #d4d4d4;}
.search-list li a{display:block;}
.search-list span{float:right;margin:0 13px 0 0;}
.search-list label{margin:0 0 0 9px;}
.search-list .all{color:#bcbcbc;}
form.adsearch p.input{margin:7px;border:1px solid #d4d4d4;background:#fff;box-shadow:0 2px 2px #dadada inset;padding:10px 7px;}
form.adsearch input[type="text"]{border:0;width:100%;color:#bcbcbc;}
form.adsearch  p.btn{text-align:center;margin:3px 8px;border:1px solid #6b972c;background:#8fba41;background-image:-moz-linear-gradient(top, #9bc540, #7aa743);background-image:-webkit-linear-gradient(top, #9bc540, #7aa743);}
form.adsearch  p.btn input[type="submit"]{width:100%;border:0;background:none;display:block;height:35px;line-height:35px;text-align:center;margin:auto;color:#fff;}
.search-list2 li{background:#e8e8e8;height:40px;line-height:40px;}
.search-list2 li a{line-height:38px;display:block;border-bottom:1px solid #cdcdcd;box-shadow:0 1px #f4f4f4;padding:0 0 0 16px;}
form.mileage{padding:3px 0 10px 0;}
form.mileage input[type="text"]{margin:7px;border:1px solid #d4d4d4;background:#fff;box-shadow:0 2px 2px #dadada inset;padding:10px 7px;display:inline-block;width:31%;}
/*地区选择*/
.search form.other{clear:both;margin-top:5px;box-shadow:0 1px 1px #b5b5b5;}
.area-list{clear:both;display:block;margin-top:12px;}
.area-list dl{overflow:hidden;background:#e8e8e8;border-top:1px solid #c7c7c7;}
.area-list dt{height:37px;background:#ddd;border-bottom:1px solid #c6c6c6;padding:0 0 0 14px;line-height:37px;box-shadow:0 1px #fff;font-size:1.25em;}
.area-list .sub-title{color:#717070;font-size:1em;}
.area-list dd{float:left;height:39px;line-height:39px;width:25%;text-align:center;}
.area-list dd a{display:block;border-right:1px solid #cdcdcd;border-bottom:1px solid #cdcdcd;box-shadow:1px 1px #f4f4f4;height: 39px;overflow: hidden;}
.area-list .letter dd{width:16.66%;}
/*车源列表*/
.search-cate li{overflow:hidden;line-height:2;border-bottom:1px solid #cdcdcd;box-shadow:0 1px #f4f4f4;padding:6px 0;}
.search-cate li .l{width:45px;float:left;text-align:right;margin-right:3px;}
.search-cate li .r{width:70%;overflow:hidden;float:left;margin:0 0 0 1%;padding-right:10%;color:#c6c6c6;position:relative;}
.search-cate li .r a{padding:0 1%;margin:0 5px;display:inline-block;}
.search-cate li .r .current{background:#39f;color:#fff;}
.search-cate li .r .more{color:#8d8d8d;}
.search-cate li .r span{position:absolute;right:0;top:10px;}
.search-cate li .r span a{display:block;width:12px;height:7px;text-indent:-82982em;}
.search-cate li .todown{height:28px;}
.search-cate li .todown span a{background:url(down.png) no-repeat 0 0;background-size:12px auto;}
.search-cate li .toup span a{background:url(up.png) no-repeat 0 0;background-size:12px auto;}
.search-cate .expand{height:37px;margin:-3px 0;}
.search-cate .expand a{display:block;height:37px;line-height:37px;text-align:center;}
.search-cate .expand a span{padding:0 10px 0 0;background:url(expand-icon.png) no-repeat right center;background-size:8px auto;}
.search-cate .expand-down a span{background:url(down-icon.png) no-repeat right center;background-size:8px auto;}
.search-btn{margin:3px 8px;border:1px solid #6b972c;background:#8fba41;background-image:-moz-linear-gradient(top, #9bc540, #7aa743);background-image:-webkit-linear-gradient(top, #9bc540, #7aa743);}
.search-btn a{display:block;height:35px;line-height:35px;text-align:center;color:#fff;}
/*车源详情*/
.details-pic{width:100%;padding:8px 0;margin:0 0 8px 0;height:100px;overflow:hidden;}
.details-pic ul{display: -webkit-box;}
.details-pic li{margin:0 2px;}
.details-pic li img{width:100px;}
.plan{text-align:center;clear:both;}
.plan a{width:14px;height:9px;background:url(details-plan-icon2.png) no-repeat center center;display:inline-block;background-size:auto 6px;}
.plan a.current{background-image:url(details-plan-icon1.png);}
.details-info{line-height:1.5;}
.details-tel{overflow:hidden;padding:2px 5px;background:#f9f9f9;border-top:2px solid #ff7200;}
.details-tel .info{float:left;}
.details-tel .info label{color:#8d8d8d;}
.details-tel .info strong{color:#ff7d00;}
.details-tel .tel{float:right;margin-right:5px;}
.details-tel .tel a{height:30px;line-height:30px;border:1px solid #6b972c;background:#8fba41;background-image:-moz-linear-gradient(top, #9bc540, #7aa743);background-image:-webkit-linear-gradient(top, #9bc540, #7aa743);display:inline-block;color:#fff;margin:5px;padding:0 10px;}
/*门店*/
.area-list2{background:#e8e8e8;}
.area-list2 ul{overflow:hidden;}
.area-list2 li{float:left;width:25%;height:39px;line-height:39px;text-align:center;}
.area-list2 li a{display:block;border-right:1px solid #cdcdcd;border-bottom:1px solid #cdcdcd;box-shadow:1px 1px #f4f4f4;}
.area-list2 li a:hover{background:#ddd;}
.store-list{border-top:1px solid #c7c7c7;}
.store-list li{margin:10px;background:#f9f9f9;border:1px solid #d4d4d4;overflow:hidden;min-width:300px;}
.store-list li h3{font-size:1.2em;}
.store-list li a{font-size:0.9em;width:76%;padding:10px 2% 10px;float:left;}
.store-list li a p{overflow:hidden;min-height:24px;}
.store-list li span.phone{border-left:1px #dedede solid;width:19%;float:right;background:#f5f5f5;height:80px;}
.store-list li span.phone a{padding:20px;float:none;display:block;width:22px;margin:auto;height:38px;text-indent:-82982em;background:url(phone.png) no-repeat center;background-size:22px auto;}
.store-list .tel{color:#c00;}
.store-list .tel strong{font-size:1.5em;font-weight:normal;}
.store-list .add{color:#bcbcbc;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.page{text-align:center;padding:9px 0 16px 0;}
.page a{display:inline-block;margin:0 3px;}
.page .prev{width:65px;height:30px;line-height:30px;background:url(page-bg1.png) no-repeat;background-size:auto 29px;text-align:right;padding:0 10px 0 0;}
.page .current{width:65px;height:30px;line-height:30px;background:url(page-bg2.png) no-repeat;background-size:auto 29px;text-align:left;}
.page span{display:inline-block;text-align:center;width:43px;margin:0 0 0 4px;}
.page .next{width:65px;height:30px;line-height:30px;background:url(page-bg3.png) no-repeat;background-size:auto 29px;text-align:left;padding:0 0 0 10px;}
.store-map{width:100%;padding:10px 0;overflow:hidden;text-align:center;}
.store-map img{width:320px;margin:auto;border:1px solid #c3c3c3;}
.details-part{clear:both;margin-bottom:10px;width:96%;padding:0 2%;}
.details-part h1{font-size:1.2em;font-weight:700;line-height:36px;}
.details-part h3{font-size:1.25em;font-weight:bold;}
.details-part dl{overflow:hidden;}
.details-part dt{color:#8d8d8d;float:left;margin:0 3px 0 0;}
.details-part dd{float:left;position:relative;}
.details-part dd.mile{width:71%;}
.details-part dd.postdate{width:71%;}
.details-part h4{border-bottom:1px solid #c3c3c3;font-weight:bold;margin:0 0 2px 0;}
.details-part h4 span{border-bottom:2px solid #ff7d00;margin-bottom:-1px;display:inline-block;}
.details-part dd .price{color:#c00;float:right;font-size:1.25em;position:relative;top:-3px;margin-bottom:-4px;}
.details-tel{position:fixed;bottom:0;width:100%;overflow:hidden;background:#f9f9f9;border-top:2px solid #ff7200;}
.details-tel .info{float:left;margin-left:5px;}
.details-tel .info label{color:#8d8d8d;}
.details-tel .info strong{color:#ff7d00;}
.details-tel .tel{float:right;}
.details-tel .tel a{height:30px;line-height:30px;border:1px solid #6b972c;background:#8fba41;background-image:-moz-linear-gradient(top, #9bc540, #7aa743);background-image:-webkit-linear-gradient(top, #9bc540, #7aa743);display:inline-block;color:#fff;margin:5px;padding:0 10px;}
.details-tel .l1{line-height:36px;}
.shopbottom{line-height:36px !important;}
/*底部*/
footer{text-align:center;background:#e8e8e8;border-top:1px solid #cacaca;padding:10px 0;}
footer.iscall{margin-bottom:42px;}
.footer-nav{margin-bottom:10px;}
.footer-nav a{display:inline-block;width:60px;height:24px;line-height:24px;background:#fff;border:1px solid #d8d8d8;margin:0 2px;padding:2px 2px 0;border-radius:4px;}
.footer-nav .top{background:#fff url(icon-top.png) no-repeat 5px 8px;background-size:9px;text-align:left;padding-left:15px;width:45px;}
footer .version{line-height:2;color:#d3d3d3;}
footer .version a{margin:0 3px;}
footer .version a.current{color:#717070;}
footer .copyright{line-height:2;color:#bcbcbc;font-size:0.875em;}
/*下拉层*/
ul.dropdown{display:none;position:absolute;z-index:3;left:0;top:30px;background:#fff;width:100%;}
ul.dropdown li{width:98%;padding:1%;cursor:pointer;}
ul.dropdown li span{float:right;font-size:12px;color:#999;margin-top:2px;}
ul.dropdown li:hover{background:#eee;}
ul.dropdown li.close{text-align:center;background:#fafafa;line-height:30px;color:#999;}
/*弹出层*/
.message{position:absolute;min-width:180px;width:90%;top:25%;left:5%;border:1px #ccc solid;background:#fff;border-radius:4px;z-index: 1001;}
.message h3{line-height:30px;padding:5px 5px 5px 50px;font-size:1.2em;}
.message p{font-size:1.1em;margin:40px auto;width:90%;}
.message p span{font-size:1.1em;cursor:pointer;display:inline-block;padding:5px 5px 8px;width:40%;text-align:center;border:1px #e5e5e5 solid;margin:5px 2%;}
.message p span.no{background:#fafafa;color:#666;}
.message p span.yes{background:#92BD40;border-color:#8a0;color:#fff;}
.pop-call p{text-align:center;}
.pop-call p.message-bottom{width:100%;border-top:1px #eee solid;margin:0;background:#f8f8f8;}
.pop-sale p.message-bottom{width:100%;border-top:1px #eee solid;margin:0;background:#f8f8f8;}
.pop-call h3{background:#F0F0F0 url(pop-call.png) no-repeat 20px center;background-size:auto 23px;}
.pop-sale h3{background:#F0F0F0 url(pop-sale.png) no-repeat 20px center;background-size:auto 23px;}

/*无搜索结果*/
.search-none{width:250px;margin:50px auto;padding-left:45px;background:url(search-none.png) no-repeat 0 5px;background-size:auto 36px;}
.search-none h4{font-size:18px;font-weight:700;}
.search-none p{color:#9a9a9a;}

/*page404*/
.page404{height:100px;margin:60px auto;padding-top:100px;background:url(page404.png) no-repeat center 0;background-size:82px auto;}
.page404 p{text-align:center;font-size:16px;}
.page404 .search-btn{width:80%;margin:20px auto;height:36px;padding:5px;}

/*认证图标*/
rz{margin-left:5px;display:inline-block;background:url(icon-rz.png) no-repeat 0 3px;background-size:15px auto;width:15px;}
.details-part dd.postdate rz{float:right;background:url(icon-rz.png) no-repeat center 2px;background-size:auto 28px;width:70px;height:30px;}

/*大图*/
#viewBigImagebg{width:100%;background-color:#1b1c1d;position:absolute;left:0;top:0;height:100%;z-index:99;display:none;}
#viewBigImage{width:100%;height:100%;position:absolute;left:0;top:0;z-index:100;display:none;}
.bigimg_topbar{position:relative;z-index:102;height:44px;}
.bigimg_topbar .btn_back{position:absolute;width:60px;height:44px;line-height:44px;text-align:center;color:#fff;top:0;left:0;background:url(back.png) no-repeat 0 0;background-size:auto 44px;}
.bigimg_topbar .btn_back span{display:block;width:44px;height:44px;text-indent:-82982px;}
.bigimg_box{width:100%;position:absolute;left:0;top:120px;z-index:101;overflow:hidden;}
.bigimg_box ul{display:-webkit-box;}
.bigimg_box ul li{width:100%;text-align:center;-webkit-transform:translate3d(0,0,0);}
.bigimg_box li img{vertical-align:middle;-webkit-animation-name:zoom;-webkit-animation-duration:.5s;-webkit-animation-timing-function:ease-in-out;}
#viewBigImage .plan{position:absolute;bottom:30px;text-align:center;width:100%;}

.ad-top{display:-webkit-box;-webkit-box-flex:1;height:115px;text-align:center;background:#F5EFE3;text-align:center;}
.ad-top a{display:block;margin:auto;width:100%;}
.ad-top img{width:320px;height:115px;}
.ad-top2{width:320px;margin:auto;height:60px;background:#fff;position:relative;}
.ad-top img{width:320px;height:60px;}
.ad-top2 span.close{font-family:"黑体";cursor:pointer;display:block;position:absolute;right:5px;top:5px;width:30px;height:30px;background:url(ad-top2-close.png) no-repeat 0 0;background-size:30px auto;text-indent:-82982em;}

.version em a{color:#3D84D0;font-style:normal;}
uptop a{text-indent:-82982em;position:fixed;bottom:20%;right:10px;display:block;width:46px;height:46px;background:url(uptop.png) no-repeat 0 0;background-size:46px auto;}

span.showmap a{display:block;font-size:1.2em;height:30px;line-height:30px;text-align:center;background:#f6f6f6;border:1px #d5d5d5 solid;margin:10px;}

.search-history{background:#fff;}
.search-history ul li{position:relative;background:url(icon-search-histroy.png) no-repeat 10px center;background-size:18px auto;text-indent:24px;line-height:36px;padding:1px 0;border-bottom:1px #ddd solid;}
.search-history ul li a{display:block;padding:0 10px;}
.search-history ul li a:hover{background:#eee url(icon-search-histroy.png) no-repeat 10px center;background-size:18px auto;}
.search-history ul li a em{position:absolute;right:10px;top:14px;display:block;width:13px;height:13px;background:url(icon-search-em.png) no-repeat 0 0;background-size:13px auto;}
span.clear-history a{display:block;width:50%;margin:10px auto;border:1px #ddd solid;background:#fff;padding:10px 0;text-align:center;font-size:1.1em;}
#noshop-area{margin-top:0;}
#noshop-area dt{display:none;}
#noshop-area dd.on a{background:#dedede;box-shadow:1px 1px 1px #ddd;}
.noshop{border-top:1px #ddd solid;padding:160px 0 120px;background:url(page404.png) no-repeat center 60px;background-size:82px auto;}
.noshop p{text-align:center;color:#999;}
.noshop p strong{font-size:1.2em;color:#333;}


#viewmap{position:absolute;top:0;left:0;width:100%;height:100%;z-index:100;}
#viewmap .contact{position:absolute;width:90%;margin:0 5%;-moz-opacity:0.95;opacity:0.95;}
#viewmap .contact span{background:#333;display:block;float:left;width:80%;height:42px;color:#bbb;border-right:1px #666 solid;padding:10px 2%;}
#viewmap .contact span strong{display:block;color:#fff;}
#viewmap .contact a{text-indent:-82982em;float:left;width:15%;height:62px;display:block;background:#333 url(phone-white.png) no-repeat center;background-size:30px auto;}
#viewmap .contact i{display:block;width:20px;height:14px;background:url(arrow-down-black.png) no-repeat center;position:relative;top:62px;}
#viewmap .bigimg_topbar{position:absolute;top:10px;left:10px;-moz-opacity:0.8;opacity:0.8;}
.infoBox img{display:none;}