body { font-family: "宋体"; font-size: 12px; color: #494949;overflow-x:hidden;}
body,h1,h2,h3,h4,h5,h6,p,ul,li,dl,dt,dd,form,input,select,button { margin: 0px; padding: 0px;}
img { border: 0;}
ul,li { list-style-type: none;}
input,select,button { font-family: "宋体"; font-size: 12px;}
a:link,a:visited { color: #000; text-decoration: none; color: #494949;}
a:hover { text-decoration: underline;}
.clear { clear: both; height: 0; font-size: 0;}
hr { margin: 0; *margin: -7px 0 -7px 0; height: 0; border: none; border-top: 1px solid #f00; display: block;}

/*清浮动代码*/
/* Clear Fix */
.clearfix:after { content:"."; display:block; height:0; clear:both; visibility:hidden;}
.clearfix { display:inline-block;}
/* Hide from IE Mac */
.clearfix { display:block;}
/* End hide from IE Mac */
/* end of clearfix */



/*共用样式*/
.lay1 { width: 990px; margin: 0 auto;}
.title_style { background: url(../images/title_bg.gif) repeat-x left bottom; height: 34px;}
.title_style h3 { float: left; display: inline; color: #494949; font-size: 14px; font-family: Microsoft YaHei;}
.title_style h3 strong { display: inline-block; height: 32px; line-height: 32px; padding: 0 21px; border-bottom: 2px solid #aaa;}
.title_style h3 a { display: inline-block; height: 32px; line-height: 32px; padding: 0 21px; border-bottom: 2px solid #aaa; cursor: pointer; position: relative;}
.title_style h3 .current { color: #f37c06; border-bottom: 2px solid #f37c06; background: url(../images/title_bg2.gif) no-repeat center bottom;}
.title_style h3 .current:link, .title_style h3 .current:visited { color: #f37c06;}
.title_style h3 a:hover { text-decoration: none;}
.title_style h3 a em { position: absolute; top: -3px; right: -1px;}
.title_style .more { float: right; display: inline; line-height: 32px; margin: 0 10px 0 0;}
.gg1{z-index:0;width:990px;margin:auto;}

.title_style .link_icon { position: relative; top: 10px;}

#sub_right .title_style strong { padding: 0 16px;}
#sub_right .title_style .current { background: none;}

.link_icon { float: right; display: inline; background: #fff; padding: 0 5px;}
.link_icon a { background: url(../images/link_icon_icon2.gif) no-repeat; width: 8px; height: 8px; display: inline-block; margin: 0 5px;}
.link_icon .current { background-image: url(../images/link_icon_icon1.gif);}

.car_list { padding: 13px 0 0 10px;}
.car_list li { width: 164px; float: left; display: inline; margin: 9px 25px 0 0; line-height: 20px; color: #a3a3a3;}
.car_list li .pic { width: 164px; height: 124px; margin: 0 0 4px 0; position: relative;}
.car_list li img { width: 160px; height: 120px; padding: 1px; border: 1px solid #ebebeb;}
.car_list li p { height: 20px; overflow: hidden;}
.car_list li span { float: right; display: inline; color: #d00; font-size: 14px; font-weight: bold; margin: 0 2px 0 0;}
.car_list .last { margin-right: 0;}
.car_list .num_bg { width: 30px; height: 18px; background: #000; filter: Alpha(Opacity=50); opacity:0.5; position: absolute; top: 2px; right: 0;}
.car_list .num { width: 30px; height: 18px; line-height: 18px; text-align: center; position: absolute; top: 2px; right: 0;}
.car_list .num span { background: url(../images/car_list_icon.gif) no-repeat left center; display: inline-block; color: #dee0de; padding: 0 0 0 14px; font-weight: normal; font-family: Arial; font-size: 12px;}

.gg2 { margin: 0 0 10px 0;}
.gg2 img { width: 750px; height: 90px;}
.gg3 { margin: 0 0 10px 0;}
.gg3 img { width: 230px; height: 90px;}

.right_content_style { background: #fcfcfc; border: 1px solid #ececec; border-top: 0;}
.right_content_style ul { zoom: 1;}

.man_list li { border-bottom: 1px solid #ebebeb;}
.man_list em { display: inline-block; width: 14px; height: 14px; text-align: center; line-height: 14px; background: #bcbcbc; color: #fff; float: left; font-style: normal; margin: 11px 0 0 14px;}
.man_list .org { background: #f37c06;}
.man_list .d1 { padding: 4px 0 0 0; height: 100px; _height: 98px;}
.man_list .pic { float: left; display: inline; margin: 11px 0 0 8px;}
.man_list .pic img { width: 60px; height: 70px; padding: 1px; border: 1px solid #ebebeb;}
.man_list .font { float: left; display: inline; line-height: 20px; margin: 11px 0 0 6px; padding: 6px 0 0 0;}
.man_list .font a { font-weight: bold;}
.man_list .font span { color: #a3a3a3;}
.man_list .font strong { color: #f37c06; margin: 0 9px;}
.man_list .d2 { height: 37px; line-height: 37px; _height: 35px; _line-height: 35px;}
.man_list .name { float: left; display: inline; margin: 0 0 0 9px; color: #a3a3a3;}
.man_list .num { float: right; display: inline; color: #a3a3a3; font-weight: bold; margin: 0 18px 0 0;}
.man_list .last { border: 0;}

.com_list li { width: 180px; margin: 0 auto; border-bottom: 1px dashed #e1e1e1; padding: 13px 0 11px 0;}
.com_list .last { border: 0;}

.com_pic { width: 164px; height: 124px; position: relative; margin: 0 auto 4px auto;}
.com_pic img { width: 160px; height: 120px; padding: 1px; border: 1px solid #ebebeb;}
.com_pic .num_bg { width: 30px; height: 18px; background: #000; filter: Alpha(Opacity=50); opacity:0.5; position: absolute; top: 2px; right: 2px;}
.com_pic .num { width: 30px; height: 18px; line-height: 18px; text-align: center; position: absolute; top: 2px; right: 0;}
.com_pic .num span { background: url(../images/car_list_icon.gif) no-repeat left center; display: inline-block; color: #dee0de; padding: 0 0 0 14px; font-weight: normal; font-family: Arial; font-size: 12px;}

.com_font { line-height: 20px; width: 164px; margin: 0 auto;}
.com_font h5 { font-size: 12px; font-weight: normal;}
.com_font p { color: #a3a3a3;}
.com_font p strong { float: right; display: inline; font-size: 14px; color: #d00;}

.hot_box .title_style { background-image: url(../images/title_bg3.gif);}


#top_nav { background: #f8f8f8; border-bottom: 1px solid #e7e7e7; height: 27px; line-height: 27px; margin: 0 0 10px 0;}
#top_nav .tel { float: left; display: inline; color: #a3a3a3;}
#top_nav .tel strong { color: #f37c06;}
#top_nav .nav { float: right; display: inline;}
#top_nav .nav span{ background: url(../images/top_nav_line.gif) no-repeat right center; display: inline-block; padding: 0 15px; text-align: center;}
#top_nav .nav span.dropdown a  { padding: 0 13px 0 0; background: url(../images/top_nav_icon.gif) no-repeat right center;}
.top_nav{position:relative;}

#head {height: 92px;}
#head .logo { float: left; display: inline; padding: 26px 0 0 6px;}
#head .city { float: left; display: inline; margin: 0 0 0 17px; padding: 18px 0 0 0;position:relative;}
#head .city .current a { background: url(../images/head_city_bg1.gif) no-repeat; display: inline-block; width: 97px; height: 33px; border: 1px solid #e7e7e7; line-height: 33px; text-align: center;}
#head .city .current a.on{ background: url(../images/head_city_bg2.gif) no-repeat; border: 1px solid #e97909; color: #fff; text-decoration: none;}

#head .city .citylist{display:none;line-height:24px;position:absolute;z-index:1;background:#f9f9f9;width:460px;top:52px;left:0;border:2px #F37C06 solid;padding:18px;}
#head .city .citylist span{display:inline-block;width:50px;height:24px;overflow:hidden;}
#head .city .citylist span a{margin:0 10px;padding:2px;}
#head .city .citylist span.on a{background:#F37C06;padding:2px;color:#fff;}
#head .city .citylist span.more{float:right;width:90px;}
#head .city .citylist span.more a{color:#f37c06;}
#head .city .citylist li.side{border-top:1px #e7e7e7 dashed;padding-top:10px;}

#daohang,#chewuzhi{display:none;position:absolute;box-shadow: #ccc 1px 1px 5px;top:26px;right:121px;z-index:2;width:215px;border:1px solid #d3d3d3;background:#fff;padding:10px;color:#6d6d6d;}
#chewuzhi form{width:210px;}
#chewuzhi h5{font-size:12px;color:#494949;font-family:"宋体";font-weight:bold;border-bottom:1px dotted #ccc;padding:0 0 3px 5px;}
#chewuzhi p.des{padding:14px 0 10px 5px;border-bottom:1px dotted #ccc;}
#chewuzhi p.mail{padding:11px 0 0 0;margin-bottom:15px;}
#chewuzhi p.mail input{border:1px solid #a7a6aa;width:183px;padding:2px;font-family:Arial;color:#aaa;margin:6px 0 7px 0;}
#chewuzhi p.mail button{width:68px;height:21px;line-height:21px;border:0;background:#ee7005;color:#fff;font-weight:bold;text-align:center;cursor:pointer;}
#pop-chewuzhi .share{clear:both;}
#daohang{width:400px;right:0;}
#daohang ul{float:left;margin-left:10px;}
#daohang ul li{width:70px;height:24px;font-weight:700;overflow:hidden;}
#daohang ul li a{font-weight:100;}

#head .search { float: right; display: inline; width: 543px; padding: 18px 0 0 0;}
#head .search_box { float: left; display: inline; width: 465px; height: 35px; background: url(../images/search_bg.gif) no-repeat;}
#head .search_box input { padding:5px 10px; margin:5px 0 0 30px;font-size: 14px; color: #a3a3a3; border: 0; width: 329px; float:left;}
#head .search_box button { float:left;background: #f37c06 url(../images/nav_bg.gif) repeat-x; width: 86px; height: 35px; border: 0;font-family: "微软雅黑"; font-size: 14px; color: #fff; cursor: pointer;}
#head .search .s1 .ad { float: left; display: inline; line-height: 35px; margin: 0 0 0 15px;}
#head .search .s2 { padding: 7px 0 0 15px; clear: both;}
#head .search .s2 a { margin: 0 12px 0 0;}
#head .search .s2 .hot { color: #f37c06;}

#nav { height: 35px; background: url(../images/nav_bg.gif) repeat-x; border-left: 1px solid #e47506; border-right: 1px solid #e47506; width: 988px; line-height: 35px; margin-bottom: 10px;}
#nav .left { float: left; display: inline;}
#nav .left a { background: url(../images/nav_a_bg.gif) no-repeat right center; font-size: 14px; font-family: Microsoft YaHei; color: #fff; font-weight: bold; padding: 0 21px; line-height: 35px; display: inline-block; position: relative;}
#nav .left .last { background: none;}
#nav .left .current, #nav .left a:hover { background: #e74c00; text-decoration: none;}
#nav .left .new { padding-right: 27px; zoom: 1;}
#nav .left a span { position: absolute; top: -7px; right: 4px;}
#nav .right { float: right; display: inline;}
#nav .right a {color:#FFE9D3;background: url(../images/nav_a_bg2.gif) no-repeat right center; padding: 0 16px;}
#nav .right .last { background: none;}

#super { background: url(../images/super_bg.gif) no-repeat; height: 107px; padding: 0 0 0 29px; width: 961px; clear: both; margin-bottom: 7px;}
#super p { float: left; display: inline; width: 189px; line-height: 20px; color: #a3a3a3; padding: 55px 0 0 54px;}
#super p.last { width: 175px;}

#friend_link { margin-bottom: 10px;}
#friend_link .content { background: #fcfcfc; border: 1px solid #ececec; border-top: none; color: #d2d2d2; line-height: 24px; padding: 13px 22px;}
#friend_link .content a{display:inline-block;word-break:keep-all;}
#friend_link .content span { margin: 0 6px;}

#quick_nav { padding: 17px 0 10px 0; margin-bottom: 10px;}
#quick_nav dl { float: left; display: inline; padding: 0 0 0 49px; background: url(../images/quick_nav_icon1.gif) no-repeat left top; line-height: 24px; width: 99px;}
#quick_nav dt { font-size: 14px; font-family: Microsoft YaHei; font-weight: bold;}
#quick_nav .d1 { width: 191px; margin: 0 0 0 10px;}
#quick_nav .d2 { background-image: url(../images/quick_nav_icon2.gif);}
#quick_nav .d3 { background-image: url(../images/quick_nav_icon3.gif);}
#quick_nav .d4 { background-image: url(../images/quick_nav_icon4.gif);}
#quick_nav .d5 { background-image: url(../images/quick_nav_icon5.gif);}
#quick_nav .d6 { background-image: url(../images/quick_nav_icon6.gif);}
#quick_nav .tel { color: #f37c06; font-size: 24px; font-family: Arial; font-weight: bold; padding: 5px 0;}
#quick_nav .time { font-size: 14px; font-family: Microsoft YaHei; font-weight: bold;}

#foot { clear: both; text-align: center; line-height: 24px; border-top: 1px solid #ebebeb; padding: 11px 0; font-family: Arial, "songti";}
#foot span { color: #d2d2d2; margin: 0 5px;}
#foot strong { color: #f37c06;}

/*表单*/
.iform li{position:relative;clear:both;}
.iform dl.select{border:1px #cecece solid;background:#fff;cursor:pointer;line-height:24px;position:absolute;left:50px;}
.iform dl.select dt{background:url(../images/icons.png) no-repeat right 10px;margin:0 9px;position:relative;z-index:0;}
.iform dl.select dd{display:none;height:24px;overflow:hidden;}
.iform dl.click dd{display:block;}
.iform dl.select dd a{display:block;margin:0;padding:0 9px;background:#fff;}
.iform dl.select dd a:hover{background:#efefef;text-decoration:none;}
.iform em{font-style:normal;line-height:24px;}
.iform ul.select{border:1px #eee solid;padding:4px;background:#fff;position:absolute;top:0;}

/*子页共用*/
#sub_left { width: 780px; float: left; display: inline;}
#sub_right { width: 200px; float: right; display: inline;}

#bread { height: 32px; line-height: 32px; padding: 0 0 0 10px;}
#bread span { margin: 0 7px; color: #a3a3a3;}
#bread strong { color: #f48701; font-weight: normal;}

#page { text-align: center; font-size: 14px; line-height: 26px; color: #a3a3a3; padding: 23px 0 4px 0; margin: 0 0 10px 0;}
#page a,#page span.a1, #page em, #page .s1 { display: inline-block; width: 26px; height: 26px; line-height: 26px; text-align: center; background: #f6f6f6; border: 1px solid #cecece; margin: 0 5px 0 0;}
#page em { font-style: normal; background: #ff8400; border: 1px solid #f07c00; color: #fff; font-weight: bold;}
#page .s1, #page .a1,#page span.a1 { width: auto; padding: 0 11px 0 12px;}
#page .s1 { background: #fafafa; border: 1px solid #e6e6e6; color: #c6c6c6;}
#page .s3 { font-size: 12px; margin: 0 0 0 11px;}
#page .s3 strong { color: #ff8400;}
#page a:hover,#page span.a1:hover { background: #ffecd7; border: 1px solid #f8c99a; color: #f37c06; text-decoration: none;cursor: pointer;}
