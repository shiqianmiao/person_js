@charset "utf-8";

audio,canvas,video{
    display:inline-block;
    *display:inline;
    *zoom:1;
}

audio:not([controls]){
    display:none;
    height:0;
}

[hidden]{
    display:none;
}

html,button,input,select,textarea{
    font-size:100%;
}
html{
    -webkit-text-size-adjust:100%;
    -ms-text-size-adjust:100%;
}
body,div,dl,dt,dd,ul,ol,menu,li,
h1,h2,h3,h4,h5,h6,
p,pre,code,
form,fieldset,legend,button,input,select,textarea,blockquote,figure{
    margin:0;
    padding:0;
}
a:active{
    outline:none!important;
    background:none;
}
a{
    text-decoration:none;
    color:#333;
}
a:visited{
    color:#333;
}
a:focus,a:active,a:hover{
    outline:none;
}

h1,h2,h3,h4,h5,h6{
    font-size:100%;
    font-weight:normal;
}

sub,sup{
    position:relative;
    vertical-align:baseline;
}

ul,ol,li{
    list-style:none;
    list-style-image:none
}


img{
    border:0;
    margin:0;
    padding:0;
    -ms-interpolation-mode:bicubic;
    max-width:100%;
}

fieldset{
    border:0;
}
legend{
    border:0;white-space:normal;*margin-left:-7px;
}
button,input,select,textarea{
    vertical-align:baseline;
    *vertical-align:middle;
    outline:none;
}
button,input{
    line-height:normal;
    border-radius:0;
}
button,
input[type="button"],
input[type="reset"],
input[type="submit"]{
    -webkit-appearance:button;
    cursor:pointer;
    *overflow:visible;
}
button[disabled],
input[disabled]{
    cursor:default;
}

input[type="checkbox"],
input[type="radio"]{
    box-sizing:border-box;
    *height:13px;
    *width:13px;}
input[type="search"]{
    -webkit-appearance:textfield;
    -moz-box-sizing:content-box;
    -webkit-box-sizing:content-box;
    box-sizing:content-box;
}
input[type="search"]::-webkit-search-cancel-button,
input[type="search"]::-webkit-search-decoration{
    -webkit-appearance:none;
}
textarea{
    overflow:auto;
    vertical-align:top;
    resize:none;
}

table{border-collapse:collapse;
    border-spacing:0;
}

.arrow-up{
  width:0px;
  height:0px;
  border-left:4px solid transparent;
  border-right:4px solid transparent;
  border-bottom:4px solid #666;
  font-size:0px;
  line-height:0px;
}

.arrow-down{
  width:0px;
  height:0px;
  border-left:4px solid transparent;
  border-right:4px solid transparent;
  border-top:4px solid #666;
  font-size:0px;
  line-height:0px;
}

.hidden{
    display:none !important;
}
.show{
    display:block !important;
}

.row:before,.row:after,
.clearfix:before,.clearfix:after{
    content:'';
    display:table;
    clear:both;
}
.row,.clearfix{
    *zoom:1;
    .span{
        float:left;
    }
}

.shange2{
    .left{
        margin-left:2%;
        width:47% !important;
        float:left;
    }
    .right{
        margin-right:2%;
        width:47% !important;
        float:right;
    }
}

.fl{
    float:left;
}
.fr{
    float:right;
}
.ma{margin:auto;}
.mt10{
    margin-top:10px !important;
}
.mr10{
    margin-right:10px !important;
}
.ml10{
    margin-left:10px !important;
}
.mb20{
    margin-bottom:20px !important;
}
.pd10{
    padding:10px !important;
}
.posrel{
    position:relative !important;
}
.posabs{
    position:absolute !important;
}

.close,.more{
    display:block;
    position:absolute;
}

.color-999,.color-gray{
    color:#999;
}
.color-f00,.color-red{
    color:#f00;
}
.color-f50,.color-orange{
    color:#f50;
}
.color-blue{
    color:#1d75d9;
}
.font-bold{
    font-weight:bold;
    a{
        font-weight:bold;
    }
}
.font-normal{
    font-weight:100;
    a{
        font-weight:100;
    }
}

.tc{text-align:center;}
.tl{text-align:left;}
.tr{text-align:right;}

.btn{
    font-weight:1em;
    display:block;
    text-align:center;
    width:95%;
    height:30px;
    line-height:30px;
    margin:10px 2.5%;
    border:1px #C5C8CF solid;
    border-radius:5px;
    background:#fcfcfc;
}
.btn-green{
    background: none repeat scroll 0 0 #8BB631;
    border-color:#8BB631;
    color:#fff;
    border: none;
    a{
        color:#fff;
        display:block;
        width:100%;
        height:100%;
    }
}


body{
    font:0.875em/1.5 "Microsoft Yahei";
    color:#333;
    min-width:320px;
}

.banner{
    overflow:hidden;
    img{
        width:100%;
        float:left;
    }
    .close{
        cursor:pointer;
        display:block;
        position:absolute;
        right:5px;
        top:5px;
        width:30px;
        height:30px;
        background:url(banner-close.png) no-repeat right 0;
        background-size:20px auto;
        text-indent:-82982em;
    }
}

#header{
    width:100%;
    background:#f2f2f2;
    .logo-full{
        float:left;
        width:165px;
        height:30px;
        margin:10px;
        background:url(logo-full.png) no-repeat 0 0;
        background-size:auto 30px;
        a{
            display:block;
            width:165px;
            height:30px;
            text-indent:-82982em;
        }
    }
    .gps a{
        display:block;
        float:right;
        margin:15px 20px;
        background:url(icon-gps.png) no-repeat 0 3px;
        background-size:12px 15px;
        padding-left:15px;
        height:20px;
        line-height:20px;
    }
}

#search{
    background:#f2f2f2;
    .cancel{
        display:none;
    }
    form{
        width:100%;
        display:-moz-box;
        display:-webkit-box;
        display:box;
        position: relative;
        .close{
            position:absolute;
            top:20px;
            right:90px;
            display:block;
            width:15px;
            height:15px;
            background:url(banner-close.png) no-repeat 0 0;
            background-size:15px auto;
            text-indent:-82982em;
            text-align:center;
            color:#999;
        }
        input[type="search"]{
            margin:0 10px;
            box-shadow:1px 1px 1px #eee;
            border:1px #b1b1b1 solid;
            border-radius:20px;
            height:18px;
            line-height:18px;
            padding:9px 0 9px 36px;
            background:#fff url(icon-search-black.png) no-repeat 15px 10px;
            background-size:15px auto;
            display:-moz-box;
            display:-webkit-box;
            display:box;
            -moz-box-flex:1;
            -webkit-box-flex:1;
            -webkit-appearance:none;
        }
    }
}


.reback,.cancel,.otherlink{
    font-size:1.2em;
    line-height:18px;
    height:18px;
    padding-top:12px;
    margin:0 5px;
}

.page-list-head{
    .reback,.otherlink{
        position:relative;
        top:8px;
    }
}

.search-head{    
    form{
        input[type="search"]{
            margin:10px 5px!important;
        }
        .reback,.cancel,.otherlink{
            display:block;
        }
        .reback{
            background:url(reback-arrow.png) no-repeat 0 13px;
            background-size:auto 15px;
            padding-left:10px;
        }
    }
}

.search-show{
    width:100%;
    min-width:320px;
    height:100%;
    position:absolute;
    z-index:999;
    top:0;
    left:0;
    form{
        input[type="search"]{
            margin:10px 5px !important;
        }
    }
    .reback,.otherlink{
        display:none !important;
    }
    .cancel{
        display:block !important;
        font-size:1.2em;
        margin:0 30px 0 10px;
        line-height:30px;
    }
    .search-history{
        display:block;
    }
}

.search-auto,.search-history{
    display:none;
    ul{
        background:#fff;
    }
    li{
        position:relative;
        background:url(icon-search-histroy.png) no-repeat 10px center;
        background-size:18px auto;
        text-indent:24px;
        line-height:36px;
        padding:1px 0;
        border-bottom:1px #ddd solid;
        a{
            display:block;
            padding:0 10px;
        }

        a:hover{
            background:#eee url(icon-search-histroy.png) no-repeat 10px center;
            background-size:18px auto;
        }

        em{
            position:absolute;
            right:10px;
            top:10px;
            display:block;
            width:23px;
            height:23px;
            background:url(icon-search-em.png) no-repeat center;
            background-size:13px auto;
        }
    }
}

.search-auto{
    position:absolute;
    width:100%;
    top:55px;
    left:0;
    ul{
        position:relative;
    }
    span{
        position:absolute;
        right:10px;
        top:0;
        color:#999;
        font-size:12px;
    }
    span.btn{
        position:relative;
        margin:10px auto;
        left:0;
        width:90%;
    }
}

.type-select{
    position:relative;
    background:#fff;
    width:320px;
    margin:auto;
    div.types{
        width:25%;
        float:left;
    }
        .type-click a{
            font-size:12px;
            width:100%;
            float:left;  
            text-align:center;
            padding:45px 0 5px;
            border-right:1px #ddd solid;
            border-bottom:1px #eee solid;
        }

        h3{
            padding-left:10px;
            width:300px;
            margin:10px 0;
        }

        .type-select-h3-price{
            border-left:5px #f37e0b solid;
        }

        .type-select-h3-brand{
            border-left:5px #4881c9 solid;
        }

        .type-select-h3-cartype{
            border-left:5px #48c242 solid;
        }

        .type-select-list{
            background:#fff;
            overflow:hidden;
            table{
                width:321px;
                background:#ddd;
                border-top:1px #ddd solid;
                border-collapse: inherit;
