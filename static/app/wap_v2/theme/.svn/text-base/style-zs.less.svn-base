	p.p1{font-size: 1.2em;}
	img{
		margin: 0;
		padding: 0;
		border: 0;
		max-width: 100%;
	}
}
.warm-tip{
	background: url("icon-note-bg.png") repeat-x scroll right center / auto 30px rgba(0, 0, 0, 0);
