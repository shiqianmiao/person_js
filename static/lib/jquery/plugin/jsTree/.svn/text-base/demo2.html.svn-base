<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>jsTree</title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<script src='http://static.273.cn/g.js' type="text/javascript"></script>
<script src='http://static.273.cn/config.js' type="text/javascript"></script>
</head>
<body>
<div id="demo">
<ul>
    <li>
        <ins>&nbsp;</ins>
        <a href="#"><ins>&nbsp;</ins>Root node 1</a>
        <ul>
            <li>
                <ins>&nbsp;</ins>
                <a href="#"><ins>&nbsp;</ins>Child node 1</a>
            </li>
            <li>
                <ins>&nbsp;</ins>
                <a href="#"><ins>&nbsp;</ins>Child node 2</a>
                <ul>
                    <li>
                        <ins>&nbsp;</ins>
                        <a href="#"><ins>&nbsp;</ins>Child node 3</a>
                    </li>
                    <li>
                        <ins>&nbsp;</ins>
                        <a href="#"><ins>&nbsp;</ins>Child node 4</a>
                    </li>
                </ul>
            </li>
        </ul>
    </li>
    <li>
        <ins>&nbsp;</ins>
        <a href="#"><ins>&nbsp;</ins>Root node 2</a>
    </li>
</ul>
</div>
<script type="text/javascript">

G.use(['lib/jquery/plugin/jsTree/jstree.js'], function () {

    $("#demo").jstree({

        "themes" : {
            "theme" : "default",
            "dots" : false,
            "icons" : false
        },

        "plugins" : [ "themes", "html_data" ]

    });
});

</script>
</body>
</html>