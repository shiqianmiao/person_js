<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>iframe-rpc</title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<script src='http://static.273.cn/g.js' type="text/javascript"></script>
<script src='http://static.273.cn/config.js' type="text/javascript"></script>
</head>
<body>
<h1>static.273.cn --- iframe2</h1>
<script type="text/javascript">
G.use(['jquery', 'lib/rpc/rpc.js'], function ($, RPC) {


    var rpc = new RPC({
        remote : parent
    });

    rpc.register('iframeTwo', function () {
        console.log('iframeTwo: ', arguments);
        return 'iframeTwo result';
    });

    rpc
        .call('hostTwo', ['a', 'b', 'c'])
        .done(function (data) {
            console.log(data);
        })
        .fail(function (data) {
            console.log(data);
        });

    window.$ = $;
    window.rpc = rpc;
});
</script>
</body>
</html>
